{
  "version": "0.0.1",
  "author": "yixi",
  "scripts": {
    "start": "yarn dev",
    "dev": "webpack-dev-server --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js",
    "test": "jest",
    "lint": "yarn tslint --project tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,json,md}": [
      "yarn lint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "dayjs": "^1.8.31",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.19",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.18.3",
    "react-router-dom": "^5.2.0",
    "swr": "^0.1.16"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.158",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-grid-layout": "^0.17.2",
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.3",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
