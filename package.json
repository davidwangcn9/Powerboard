{
  "version": "2.0.0",
  "author": "guhzongren",
  "name": "Powerboard",
  "scripts": {
    "dev": "webpack serve --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js",
    "test": "jest",
    "fix": "prettier -w . && eslint src/**/*.tsx --fix && eslint src/**/*.ts --fix",
    "lint": "prettier -c . && eslint src/**/*.tsx && eslint src/**/*.ts",
    "release": "standard-version --release-as patch",
    "minorRelease": "standard-version --release-as minor",
    "majorRelease": "standard-version --release-as major",
    "cy:run": "cross-env ENV=production cypress run --spec 'cypress/integration/dashboard.spec.js'",
    "cy:docker": "cross-env ENV=docker cypress run --spec 'cypress/integration/dashboard.spec.js'",
    "cy:open": "cypress open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*{.ts,.tsx,.yml}": [
      "lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.4.5"
  },
  "dependencies": {
    "cypress": "^9.5.2",
    "dayjs": "^1.11.0",
    "graphql": "^16.3.0",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.4",
    "react-router-dom": "^6.2.2",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-grid-layout": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress-downloadfile": "^1.2.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.6",
    "mini-css-extract-plugin": "2.4.5",
    "node-sass": "^7.0.1",
    "pinst": "^2.1.6",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "sass-loader": "^12.6.0",
    "standard": "^16.0.4",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "workbox-webpack-plugin": "^6.5.1"
  }
}
